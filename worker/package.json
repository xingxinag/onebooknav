{
  "name": "onebooknav-worker",
  "version": "1.0.0",
  "description": "OneBookNav Cloudflare Workers 边缘计算部署",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --local",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "build": "rollup -c rollup.config.js",
    "build:worker": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "logs": "wrangler tail",
    "kv:create": "wrangler kv:namespace create",
    "d1:create": "wrangler d1 create onebooknav",
    "d1:migrations": "wrangler d1 migrations apply onebooknav",
    "r2:create": "wrangler r2 bucket create onebooknav-files"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20241011.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "wrangler": "^3.78.12"
  },
  "keywords": [
    "cloudflare-workers",
    "edge-computing",
    "bookmark-manager",
    "navigation",
    "serverless"
  ],
  "author": "OneBookNav Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/onebooknav.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/onebooknav/issues"
  },
  "homepage": "https://github.com/your-repo/onebooknav#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ]
}